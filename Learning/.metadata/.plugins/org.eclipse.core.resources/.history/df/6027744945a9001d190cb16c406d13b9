package com.self.practise.program;


//Base class or Parent class
public class AnimalsuperKeyword1 {
	
	public static String animalType="black";
	
	// Dog is child class of AnimalsuperKeyword1
	public class Dog extends AnimalsuperKeyword1{
		
	    //default constructor  
		Dog(){
			String color="White";
			System.out.println("Dog color is :" +super.animalType);
			
		}
		
		public class Test extends Dog{
			
			//default constructor 
			Test(){
				super();
				System.out.println("Dog is wild animal");  
				
			}
		}
		
		
}
	 public static void main(String [] args) {
		 AnimalsuperKeyword1 ak1 = new AnimalsuperKeyword1();
		 
		 try {
			System.out.println("Dog color is :" +animalType);
		} catch (Exception e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
		 
	 }
	
	
}
